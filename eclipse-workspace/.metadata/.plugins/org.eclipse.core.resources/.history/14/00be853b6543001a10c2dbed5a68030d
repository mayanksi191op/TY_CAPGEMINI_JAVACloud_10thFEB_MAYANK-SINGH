
public class AssignTest {

	public static void main(String[] args) {
		Student[] s=new Student[5];
		s[0].setHeight(6.1);
		s[0].setId(1);
		s[0].setName("Mayank");
		
		s[1].setHeight(5.2);
		s[1].setId(2);
		s[1].setName("Pankaj");
		
		s[2].setHeight(5.4);
		s[2].setId(3);
		s[2].setName("Lodu");
		
		s[3].setHeight(6.2);
		s[3].setId(4);
		s[3].setName("Sonu");
		
		s[4].setHeight(5.2);
		s[4].setId(5);
		s[4].setName("Chomu");
		
		for(int i=0; i<5; i++)
		{
			int k;
			k=i+1;
			System.out.println("DETAILS OF "+k+"STUDENT");
			System.out.println(s[i].getHeight());
			System.out.println(s[i].getId());
			System.out.println(s[i].getName());
		}
				
				
				
	}
}
